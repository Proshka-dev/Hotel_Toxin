{
  "compilerOptions": {
    "noImplicitAny": true,
    "target": "es2015",
    "moduleResolution": "nodenext",
    "module": "nodenext",

    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "strictFunctionTypes": true,
    "noImplicitThis": true,
    "strictBindCallApply": true
  }
}
