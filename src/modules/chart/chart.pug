//- Объявление миксина
mixin mixChart()
	.chart
		svg.chart__body
			circle(r='50' cx='50' cy='50' fill='hsla(5, 100%,50%,.5)')
			circle(r='50' cx='130' cy='50' fill='hsla(50, 100%,50%,.5)')
			circle(r='50' cx='50' cy='130' fill='hsla(90, 100%,50%,.5)')
			circle(r='50' cx='130' cy='130' fill='hsla(200, 100%,50%,.5)')